uint8 NUT = 1
uint8 NDT = 2
uint8 NUB = 4
uint8 NDB = 8
uint8 FUT = 16
uint8 FDT = 32
uint8 FUB = 64
uint8 FDB = 128

int8 ANY_REV_COUNT = -100

